{
  "name": "todoApp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "watch": "npm-watch",
    "test": "mocha-webpack --webpack-config webpack.config.js --require test/setup.js *.spec.js --recursive test"
  },
  "dependencies": {
    "cross-env": "^5.1.4",
    "nyc": "^11.6.0",
    "vue": "^2.5.16",
    "vue-resource": "^1.5.0",
    "vue-resource-mock": "0.0.9",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.13",
    "expect": "^22.4.3",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.5",
    "mocha-webpack": "^1.1.0",
    "npm-watch": "^0.3.0",
    "sinon": "^5.0.2",
    "vue-template-compiler": "^2.5.13",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,vue"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
